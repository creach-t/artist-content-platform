    "docker:shell": "docker-compose -f docker-compose.dev.yml exec nextjs-app sh",
    "docker:prisma:migrate": "docker-compose -f docker-compose.dev.yml exec nextjs-app npx prisma migrate dev",
    "docker:prisma:studio": "docker-compose -f docker-compose.dev.yml exec nextjs-app npx prisma studio --browser none --port 5555",
    "docker:seed": "docker-compose -f docker-compose.dev.yml exec nextjs-app npm run db:seed"
  },
